{
  "name": "manager-tool",
  "version": "1.0.0",
  "private": true,
  "description": "Lottery 6/49 Tool",
  "author": {
    "name": "QuantaPLC",
    "email": "dev@quantaplc.im",
    "url": "https://www.quantaplc.im/"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron-builder": "20.14.7",
    "electron-devtools-installer": "2.2.4",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "foreman": "^2.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "1.3.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.1",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "bip39": "^2.5.0",
    "browserify-css": "^0.14.0",
    "crypto-js": "^3.1.9-1",
    "decimal.js": "^10.0.1",
    "electron": "2.0.2",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.4",
    "ethereumjs-units": "^0.2.0",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-wallet": "^0.6.0",
    "fast-csv": "^2.4.1",
    "font-awesome": "^4.7.0",
    "fs": "^0.0.1-security",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-ionicons": "^2.1.6",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "4.0.1",
    "sass-loader": "^7.1.0",
    "yarn": "^1.12.3"
  },
  "homepage": "./",
  "main": "src/electron.js",
  "scripts": {
    "start": "BROWSER=none node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "dev": "npm run compile && nf start -p 4000",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package-linux-win": "npm run compile && npm run build && electron-builder --linux && electron-builder --win --x64",
    "package-linux": "npm run compile && npm run build && electron-builder --linux",
    "package-win": "npm run compile && npm run build && electron-builder --win --x64",
    "package-mac": "npm run compile && npm run build && electron-builder --mac",
    "package-release": "npm run compile && npm run build && electron-builder --linux && electron-builder --win --x64 && electron-builder --mac",
    "compile": "node parser/index.js",
    "compile:sass": "node-sass ./src/views/sass/main.scss ./public/styles.css -w"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "org.develar.manager-tool",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "icon": "build/resources/manager.icns"
    },
    "nsis": {
      "oneClick": true,
      "allowElevation": false,
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "installerIcon": "build/resources/manager.ico",
      "installerHeaderIcon": "build/resources/manager.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.finance",
      "identity": "Quanta Technology Limited (CT95N7NKQQ)",
      "icon": "build/resources/manager.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/resources/manager.ico"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Development",
      "icon": "build/resources/manager"
    },
    "directories": {
      "buildResources": "build/resources",
      "output": "release"
    },
    "productName": "Manager Tool"
  },
  "resolutions": {
    "**/deep-extend": "^0.6.0",
    "**/react-scripts/webpack-dev-server": "^2.11.2"
  },
  "productName": "Manager Tool"
}